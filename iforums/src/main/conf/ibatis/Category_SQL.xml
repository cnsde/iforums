<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="Category">
	<select id="select" resultClass="net.iforums.beans.Category" parameterClass="java.util.Map">
		SELECT * FROM JFORUM_CATEGORIES ORDER BY 'ORDER' limit #startId#,#endId#
	</select>
	<select id="getObjectById" resultClass="net.iforums.beans.Category" parameterClass="java.util.Map">
		SELECT * FROM JFORUM_CATEGORIES WHERE ID=#id#
	</select>
</sqlMap>