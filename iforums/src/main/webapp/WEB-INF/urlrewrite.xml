<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 2.6//EN" "http://tuckey.org/res/dtds/urlrewrite2.6.dtd">
<!--
    Configuration file for UrlRewriteFilter http://tuckey.org/urlrewrite/
-->
<urlrewrite>
    <rule>
        <from>^/$</from>
        <to>/index.do</to>
    </rule>
    <rule>
        <from>^/index-([0-9]+)\.html</from>
        <to>/index.do?id=$1</to>
    </rule>
    <rule>
        <from>^/forum-([0-9]+)\.html</from>
        <to>/forum.do?id=$1</to>
    </rule>
    <rule>
        <from>^/topic-([0-9]+)\.html</from>
        <to>/topic.do?id=$1</to>
    </rule>
    <rule>
        <from>^/newTopic-([0-9]+)\.html</from>
        <to>/newTopic.do?id=$1</to>
    </rule>
    <rule>
        <from>^/([a-zA-Z0-9]+)\.html</from>
        <to>/$1.do</to>
    </rule>
    <rule>
        <from>^/([a-zA-Z0-9]+)/([a-zA-Z0-9]+)\.html</from>
        <to>/$1/$2.do</to>
    </rule>
</urlrewrite>